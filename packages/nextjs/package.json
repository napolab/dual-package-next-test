    "@types/prettier": "^2.7.2",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.10",
    "eslint": "^8.34.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.4",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "esbuild-cjs": "link:../esbuild-cjs",
    "esbuild-esm": "link:../esbuild-esm",
    "next": "^13.1.6",
